    "body-parser": "^1.18.3",
    "bootstrap-daterangepicker": "^3.0.3",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "express": "^4.17.3",
    "jquery": "^3.3.1",
    "jwt-simple": "^0.5.5",
    "moment": "^2.24.0",
    "mongoose": "^5.4.17",
    "morgan": "^1.9.1",
    "nodemailer": "^5.1.1",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.7.1",
    "react": "^16.8.1",
    "react-bootstrap-daterangepicker": "^4.1.0",
    "react-chips": "^0.8.0",
    "react-color": "^2.17.0",
